#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/manikanta/Desktop/GIT/ReactProjects/mentor-management-system/mentor-management-backend/node_modules/touch/bin/node_modules:/home/manikanta/Desktop/GIT/ReactProjects/mentor-management-system/mentor-management-backend/node_modules/touch/node_modules:/home/manikanta/Desktop/GIT/ReactProjects/mentor-management-system/mentor-management-backend/node_modules:/home/manikanta/Desktop/GIT/ReactProjects/mentor-management-system/node_modules:/home/manikanta/Desktop/GIT/ReactProjects/node_modules:/home/manikanta/Desktop/GIT/node_modules:/home/manikanta/Desktop/node_modules:/home/manikanta/node_modules:/home/node_modules:/node_modules:/home/manikanta/Desktop/GIT/ReactProjects/mentor-management-system/mentor-management-backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/manikanta/Desktop/GIT/ReactProjects/mentor-management-system/mentor-management-backend/node_modules/touch/bin/node_modules:/home/manikanta/Desktop/GIT/ReactProjects/mentor-management-system/mentor-management-backend/node_modules/touch/node_modules:/home/manikanta/Desktop/GIT/ReactProjects/mentor-management-system/mentor-management-backend/node_modules:/home/manikanta/Desktop/GIT/ReactProjects/mentor-management-system/node_modules:/home/manikanta/Desktop/GIT/ReactProjects/node_modules:/home/manikanta/Desktop/GIT/node_modules:/home/manikanta/Desktop/node_modules:/home/manikanta/node_modules:/home/node_modules:/node_modules:/home/manikanta/Desktop/GIT/ReactProjects/mentor-management-system/mentor-management-backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../touch/bin/nodetouch.js" "$@"
else
  exec node  "$basedir/../touch/bin/nodetouch.js" "$@"
fi
